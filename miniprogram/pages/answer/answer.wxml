<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="content">答题</view>
</cu-custom>


<view class="cu-bar bg-white solid-bottom margin">
  <view class='action'>
    <text class="cuIcon-title text-blue"> 1.{{title}}</text>
  </view>
</view>

<view wx:for='{{options}}' wx:key>
  <view class="bg-white padding-lg margin-xl radius">
    <text>{{item}}</text>
  </view>
</view>

<button>下一题</button>
<view>
  <text>共15题，第{{}}题</text>
</view>
<button>上一题</button>